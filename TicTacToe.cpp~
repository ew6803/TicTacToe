#include <iostream>
#include <string.h>
#include <cstring>

using namespace std;

int main () {
  char board[4][4];
  board[0][0] = ' ';
  board[0][1] = 'a';
  board[0][2] = 'b';
  board[0][3] = 'c';
  board[1][0] = '1';
  board[2][0] = '2';
  board[3][0] = '3';
  cout << board[0][0] << board[1][0] << board[2][0] << board[3][0] << "\n";
  cout << board[0][1] << "\n";
  cout << board[0][2] << "\n";
  cout << board[0][3];
  bool xMove;
  bool oMove;
  char input[2];
  cin >> input;
  cout << input;
  






  return 0;
}
